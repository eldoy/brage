{
  "name": "brage",
  "version": "1.0.0",
  "description": "Javascript component library concept for single page applications. No dependencies, no vdom and very compact. Includes router. 100% test coverage.",
  "main": "dist/brage.js",
  "module": "dist/brage.es.js",
  "jsnext:main": "dist/brage.es.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "start": "webpack -d --watch",
    "dev": "concurrently \"npm:start\" \"npm:server\"",
    "server": "live-server --entry-file=./index.html dist",
    "test": "NODE_ENV=test jest --watch",
    "rollup": "rollup -c"
  },
  "author": "Vidar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fugroup/brage.git"
  },
  "keywords": [
    "brage",
    "tiny",
    "dom",
    "library"
  ],
  "bugs": {
    "url": "https://github.com/fugroup/brage/issues"
  },
  "homepage": "https://github.com/fugroup/brage",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "jest": "^23.2.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-transform-stub": "^1.0.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  },
  "engines": {
    "node": "^8.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  }
}
